<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    android:id="@+id/menu_layout"
    >

    <com.github.mmin18.widget.RealtimeBlurView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:realtimeBlurRadius="3dp"
        app:realtimeDownsampleFactor="16"
        app:realtimeOverlayColor="#DD222222"/>






    <com.bullb.ctf.Widget.CircleView
        android:id="@+id/circle_view"
        android:layout_width="@dimen/circle_width"
        android:layout_height="@dimen/circle_width">

        <ImageView
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:src="@drawable/close"
            android:layout_alignParentRight="true"
            android:layout_marginRight="8dp"
            android:layout_marginTop="8dp"
            android:id="@+id/dismiss_btn"
            android:background="?attr/selectableItemBackgroundBorderless"/>

    </com.bullb.ctf.Widget.CircleView>





</RelativeLayout>
